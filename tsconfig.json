{
  "compilerOptions": {
    "outDir": "dist",
    "noImplicitAny": true,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "esnext",
    "declaration": true,
    "esModuleInterop": true,
    "lib": ["es2015", "dom"],
    "strict": true,
    "baseUrl": "./"
  },
  "include": [
    "data-structures/**/*",
    "search/**/*",
    "sorts/**/*",
    "traversal/**/*",
    "*.d.ts"
  ],
  "exclude": ["node_modules"]
}
